        "liquid_BeU7cf": {
          "type": "liquid",
          "settings": {
            "liquid": "{% comment %}\n  PDF Download Button - Universal (Works for ALL Product Templates)\n  Add this as a Custom Liquid block in your product page template\n{% endcomment %}\n\n<div class=\"product-pdf-download\" style=\"margin: 20px 0;\">\n  <button id=\"download-pdf-btn\" class=\"btn\" style=\"\n    background-color: #000;\n    color: #fff;\n    padding: 12px 24px;\n    border: none;\n    cursor: pointer;\n    font-size: 14px;\n    font-weight: 600;\n    border-radius: 4px;\n    display: inline-flex;\n    align-items: center;\n    gap: 8px;\n  \">\n    <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n      <path d=\"M8 11L3 6H6V0H10V6H13L8 11Z\" fill=\"currentColor\"/>\n      <path d=\"M0 14H16V16H0V14Z\" fill=\"currentColor\"/>\n    </svg>\n    Download Product PDF\n  </button>\n</div>\n\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js\" integrity=\"sha512-qZvrmS2ekKPF2mSznTQsxqPgnpkI4DNTlrdUmTzrDgektczlKNRRhy5X5AAOnx5S09ydFYWWNSfcEqDTTHgtNA==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\n\n<script>\nwindow.addEventListener('load', function() {\n  \n  var productData = {\n    title: {{ product.title | json }},\n    description: {{ product.description | strip_html | json }},\n    price: {{ product.price | money | json }},\n    comparePrice: {{ product.compare_at_price | money | json }},\n    vendor: {{ product.vendor | json }},\n    type: {{ product.type | json }},\n    sku: {{ product.selected_or_first_available_variant.sku | json }},\n    weight: '{{ product.selected_or_first_available_variant.weight | weight_with_unit }}',\n    images: [\n      {% for image in product.images %}\n        {{ image | image_url: width: 800 | json }}{% unless forloop.last %},{% endunless %}\n      {% endfor %}\n    ],\n    tags: {{ product.tags | json }},\n    available: {{ product.available | json }},\n    specifications: {\n      // Common fields\n      stockNumber: {{ product.metafields.custom.stock_number | json }},\n      weight: {{ product.metafields.custom.weight | json }},\n      color: {{ product.metafields.custom.color | json }},\n      clarity: {{ product.metafields.custom.clarity | json }},\n      \n      // Template 1: Diamond-specific fields\n      shape: {{ product.metafields.custom.shape | json }},\n      symmetry: {{ product.metafields.custom.symmetry | json }},\n      polish: {{ product.metafields.custom.polish | json }},\n      measurements: {{ product.metafields.custom.measurements | json }},\n      depth: {{ product.metafields.custom.depth | json }},\n      table: {{ product.metafields.custom.table | json }},\n      certificateNumber: {{ product.metafields.custom.certificate_number | json }},\n      fluorescence: {{ product.metafields.custom.fluorescence | json }},\n      \n      // Template 2: Jewelry-specific fields\n      productType: {{ product.type | json }},\n      weightInGram: {{ product.metafields.custom.weight_in_gram | json }},\n      goldColor: {{ product.metafields.custom.gold_color | json }},\n      purity: {{ product.metafields.custom.purity | json }},\n      totalDiamonds: {{ product.metafields.custom.total_diamonds | json }}\n    }\n  };\n\n  // Function to load image and convert to base64\n  function loadImage(url) {\n    return new Promise(function(resolve, reject) {\n      var img = new Image();\n      img.crossOrigin = 'Anonymous';\n      \n      img.onload = function() {\n        var canvas = document.createElement('canvas');\n        canvas.width = img.width;\n        canvas.height = img.height;\n        \n        var ctx = canvas.getContext('2d');\n        ctx.drawImage(img, 0, 0);\n        \n        try {\n          var dataURL = canvas.toDataURL('image/jpeg', 0.8);\n          resolve(dataURL);\n        } catch (e) {\n          reject(e);\n        }\n      };\n      \n      img.onerror = function() {\n        reject(new Error('Failed to load image'));\n      };\n      \n      img.src = url;\n    });\n  }\n\n  var button = document.getElementById('download-pdf-btn');\n  \n  if (!button) {\n    console.error('Button not found');\n    return;\n  }\n\n  button.addEventListener('click', async function(e) {\n    e.preventDefault();\n    \n    var originalHTML = this.innerHTML;\n    this.disabled = true;\n    this.innerHTML = 'Generating PDF...';\n\n    try {\n      if (typeof window.jspdf === 'undefined') {\n        alert('PDF library not loaded. Please refresh the page.');\n        button.disabled = false;\n        button.innerHTML = originalHTML;\n        return;\n      }\n\n      var jsPDF = window.jspdf.jsPDF;\n      var doc = new jsPDF();\n      \n      var yPosition = 20;\n      var pageWidth = doc.internal.pageSize.getWidth();\n      var margin = 20;\n      var contentWidth = pageWidth - (margin * 2);\n\n      // Store name\n      doc.setFontSize(10);\n      doc.setTextColor(100);\n      doc.text('{{ shop.name }}', margin, yPosition);\n      yPosition += 15;\n\n      // Product Title\n      doc.setFontSize(18);\n      doc.setTextColor(0);\n      doc.setFont(undefined, 'bold');\n      var titleLines = doc.splitTextToSize(productData.title, contentWidth);\n      doc.text(titleLines, margin, yPosition);\n      yPosition += (titleLines.length * 7) + 10;\n\n      // Line separator\n      doc.setDrawColor(200);\n      doc.line(margin, yPosition, pageWidth - margin, yPosition);\n      yPosition += 15;\n\n      // Add ALL Product Images\n      if (productData.images && productData.images.length > 0) {\n        doc.setFontSize(14);\n        doc.setFont(undefined, 'bold');\n        doc.text('Diamond Images', margin, yPosition);\n        yPosition += 10;\n\n        var imgWidth = 70;\n        var imgHeight = 70;\n        var imgSpacing = 5;\n        var imagesPerRow = 2;\n        var xPosition = margin;\n        var imageCount = 0;\n\n        for (var idx = 0; idx < productData.images.length; idx++) {\n          try {\n            var imageData = await loadImage(productData.images[idx]);\n            \n            // Check if we need to move to next row\n            if (imageCount > 0 && imageCount % imagesPerRow === 0) {\n              xPosition = margin;\n              yPosition += imgHeight + imgSpacing;\n            }\n            \n            // Check if we need a new page\n            if (yPosition + imgHeight > 270) {\n              doc.addPage();\n              yPosition = 20;\n              xPosition = margin;\n            }\n            \n            // Add image\n            doc.addImage(imageData, 'JPEG', xPosition, yPosition, imgWidth, imgHeight);\n            \n            // Move x position for next image in row\n            xPosition += imgWidth + imgSpacing;\n            imageCount++;\n            \n          } catch (imgError) {\n            console.log('Could not load image ' + idx + ':', imgError);\n          }\n        }\n        \n        // Move y position to after all images\n        yPosition += imgHeight + 15;\n      }\n\n      // Check if need new page before specifications\n      if (yPosition > 200) {\n        doc.addPage();\n        yPosition = 20;\n      }\n\n      // Product Specifications Section - DYNAMIC based on available fields\n      doc.setFontSize(14);\n      doc.setFont(undefined, 'bold');\n      doc.text('Diamond Specifications', margin, yPosition);\n      yPosition += 10;\n\n      // Build specifications array dynamically\n      var specs = [];\n      \n      // Add common fields\n      if (productData.specifications.stockNumber) {\n        specs.push({ label: 'Stock number', value: productData.specifications.stockNumber });\n      }\n      \n      if (productData.specifications.weight) {\n        specs.push({ label: 'Weight', value: productData.specifications.weight + ' ct' });\n      }\n      \n      // Template 1: Diamond-specific fields\n      if (productData.specifications.shape) {\n        specs.push({ label: 'Shape', value: productData.specifications.shape });\n      }\n      \n      // Template 2: Jewelry type field\n      if (productData.specifications.productType && !productData.specifications.shape) {\n        specs.push({ label: 'Type', value: productData.specifications.productType });\n      }\n      \n      if (productData.specifications.color) {\n        specs.push({ label: 'Color', value: productData.specifications.color });\n      }\n      \n      if (productData.specifications.clarity) {\n        specs.push({ label: 'Clarity', value: productData.specifications.clarity });\n      }\n      \n      // Template 1: Diamond fields\n      if (productData.specifications.symmetry) {\n        specs.push({ label: 'Symmetry', value: productData.specifications.symmetry });\n      }\n      \n      if (productData.specifications.polish) {\n        specs.push({ label: 'Polish', value: productData.specifications.polish });\n      }\n      \n      if (productData.specifications.measurements) {\n        specs.push({ label: 'Measurements', value: productData.specifications.measurements });\n      }\n      \n      if (productData.specifications.depth) {\n        specs.push({ label: 'Depth %', value: productData.specifications.depth });\n      }\n      \n      if (productData.specifications.table) {\n        specs.push({ label: 'Table %', value: productData.specifications.table });\n      }\n      \n      if (productData.specifications.certificateNumber) {\n        specs.push({ label: 'Certificate number', value: productData.specifications.certificateNumber });\n      }\n      \n      if (productData.specifications.fluorescence) {\n        specs.push({ label: 'Fluorescence Intensity', value: productData.specifications.fluorescence });\n      }\n      \n      // Template 2: Jewelry fields\n      if (productData.specifications.weightInGram) {\n        specs.push({ label: 'Weight (in grams)', value: productData.specifications.weightInGram });\n      }\n      \n      if (productData.specifications.goldColor) {\n        specs.push({ label: 'Gold Color', value: productData.specifications.goldColor });\n      }\n      \n      if (productData.specifications.purity) {\n        specs.push({ label: 'Purity', value: productData.specifications.purity });\n      }\n      \n      if (productData.specifications.totalDiamonds) {\n        specs.push({ label: 'Total Diamonds', value: productData.specifications.totalDiamonds });\n      }\n\n      // Draw specifications table only if we have specs\n      if (specs.length > 0) {\n        doc.setFontSize(10);\n        var tableWidth = contentWidth;\n        var cellHeight = 7;\n        var col1Width = tableWidth * 0.4;\n        var col2Width = tableWidth * 0.6;\n\n        // Draw table border\n        doc.setDrawColor(158);\n        doc.rect(margin, yPosition, tableWidth, specs.length * cellHeight);\n\n        for (var s = 0; s < specs.length; s++) {\n          var spec = specs[s];\n          var rowY = yPosition + (s * cellHeight);\n          \n          // Draw row border\n          doc.setDrawColor(216);\n          if (s > 0) {\n            doc.line(margin, rowY, margin + tableWidth, rowY);\n          }\n          \n          // Label (left column)\n          doc.setFont(undefined, 'normal');\n          doc.setTextColor(0);\n          doc.text(spec.label, margin + 2, rowY + 5);\n          \n          // Value (right column)\n          doc.setTextColor(96);\n          doc.text(String(spec.value), margin + col1Width + 2, rowY + 5);\n        }\n\n        yPosition += (specs.length * cellHeight) + 15;\n      }\n\n      // Product Details\n      if (yPosition > 250) {\n        doc.addPage();\n        yPosition = 20;\n      }\n\n      doc.setFontSize(14);\n      doc.setFont(undefined, 'bold');\n      doc.setTextColor(0);\n      doc.text('Product Information', margin, yPosition);\n      yPosition += 10;\n\n      doc.setFontSize(11);\n      doc.setFont(undefined, 'normal');\n\n      // Price\n      doc.setFont(undefined, 'bold');\n      doc.text('Price:', margin, yPosition);\n      doc.setFont(undefined, 'normal');\n      doc.text(productData.price, margin + 50, yPosition);\n      yPosition += 8;\n\n      // Availability\n      doc.setFont(undefined, 'bold');\n      doc.text('Availability:', margin, yPosition);\n      doc.setFont(undefined, 'normal');\n      if (productData.available) {\n        doc.setTextColor(0, 128, 0);\n        doc.text('In Stock', margin + 50, yPosition);\n      } else {\n        doc.setTextColor(255, 0, 0);\n        doc.text('Out of Stock', margin + 50, yPosition);\n      }\n      doc.setTextColor(0);\n      yPosition += 8;\n\n      // SKU\n      if (productData.sku) {\n        doc.setFont(undefined, 'bold');\n        doc.text('SKU:', margin, yPosition);\n        doc.setFont(undefined, 'normal');\n        doc.text(String(productData.sku), margin + 50, yPosition);\n        yPosition += 8;\n      }\n\n      // Vendor\n      if (productData.vendor) {\n        doc.setFont(undefined, 'bold');\n        doc.text('Brand:', margin, yPosition);\n        doc.setFont(undefined, 'normal');\n        doc.text(productData.vendor, margin + 50, yPosition);\n        yPosition += 8;\n      }\n\n      yPosition += 5;\n\n      // Description\n      if (productData.description && productData.description.trim() !== '') {\n        if (yPosition > 250) {\n          doc.addPage();\n          yPosition = 20;\n        }\n\n        doc.setFontSize(14);\n        doc.setFont(undefined, 'bold');\n        doc.text('Description', margin, yPosition);\n        yPosition += 10;\n\n        doc.setFontSize(10);\n        doc.setFont(undefined, 'normal');\n        var descLines = doc.splitTextToSize(productData.description, contentWidth);\n        \n        for (var i = 0; i < descLines.length; i++) {\n          if (yPosition > 270) {\n            doc.addPage();\n            yPosition = 20;\n          }\n          doc.text(descLines[i], margin, yPosition);\n          yPosition += 5;\n        }\n      }\n\n      // Tags\n      if (productData.tags && productData.tags.length > 0) {\n        yPosition += 10;\n        if (yPosition > 270) {\n          doc.addPage();\n          yPosition = 20;\n        }\n        doc.setFontSize(12);\n        doc.setFont(undefined, 'bold');\n        doc.text('Tags', margin, yPosition);\n        yPosition += 8;\n\n        doc.setFontSize(10);\n        doc.setFont(undefined, 'normal');\n        var tagsText = productData.tags.join(', ');\n        var tagLines = doc.splitTextToSize(tagsText, contentWidth);\n        for (var j = 0; j < tagLines.length; j++) {\n          doc.text(tagLines[j], margin, yPosition);\n          yPosition += 5;\n        }\n      }\n\n      // Footer\n      var pageCount = doc.internal.getNumberOfPages();\n      for (var p = 1; p <= pageCount; p++) {\n        doc.setPage(p);\n        doc.setFontSize(8);\n        doc.setTextColor(120);\n        doc.text('Page ' + p + ' of ' + pageCount, pageWidth - margin - 20, doc.internal.pageSize.getHeight() - 10);\n      }\n\n      // Save PDF\n      var filename = productData.title.replace(/[^a-z0-9]/gi, '_').toLowerCase().substring(0, 50) + '.pdf';\n      doc.save(filename);\n\n      button.disabled = false;\n      button.innerHTML = originalHTML;\n\n    } catch (error) {\n      console.error('PDF Error:', error);\n      alert('Error creating PDF: ' + error.message);\n      button.disabled = false;\n      button.innerHTML = originalHTML;\n    }\n  });\n});\n</script>\n\n<style>\n  #download-pdf-btn:hover {\n    opacity: 0.9;\n    transform: translateY(-1px);\n    transition: all 0.2s ease;\n  }\n\n  #download-pdf-btn:active {\n    transform: translateY(0);\n  }\n\n  #download-pdf-btn:disabled {\n    opacity: 0.6;\n    cursor: not-allowed;\n  }\n</style>"
